@model finalproject.Models.Template
@{
    ViewData["Title"] = "Delete Template";
}
<h2>Delete Template</h2>
<div>
    <h3>Are you sure you want to delete this template?</h3>
    <p>
        <strong>Title:</strong> @Model.Title <br />
        <strong>Description:</strong> @Model.Description
    </p>
</div>
<form asp-action="Delete" method="post">
    @Html.AntiForgeryToken()
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a href="@Url.Action("Index", "AdminContent")" class="btn btn-secondary">Cancel</a>
</form>
