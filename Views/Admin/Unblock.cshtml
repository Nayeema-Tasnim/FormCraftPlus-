@model finalproject.Models.ApplicationUser
@{
    ViewData["Title"] = "Unblock User";
}
<h2>Unblock User</h2>
<p>Are you sure you want to unblock the user <strong>@Model.Email</strong>?</p>
<form asp-action="Unblock" method="post">
    @Html.AntiForgeryToken()
    <input type="hidden" name="id" value="@Model.Id" />
    <button type="submit" class="btn btn-primary">Unblock User</button>
    <a href="@Url.Action("Index", "Admin")" class="btn btn-secondary">Cancel</a>
</form>
